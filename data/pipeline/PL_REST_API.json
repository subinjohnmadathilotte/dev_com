{
	"name": "PL_REST_API",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['id']"
								},
								"sink": {
									"name": "id"
								}
							},
							{
								"source": {
									"path": "$['customer_id']"
								},
								"sink": {
									"name": "customer_id"
								}
							},
							{
								"source": {
									"path": "$['customer_city']"
								},
								"sink": {
									"name": "customer_city"
								}
							},
							{
								"source": {
									"path": "$['customer_state']"
								},
								"sink": {
									"name": "customer_state"
								}
							},
							{
								"source": {
									"path": "$['customer_zip_code_prefix']"
								},
								"sink": {
									"name": "customer_zip_code_prefix"
								}
							}
						],
						"collectionReference": ""
					}
				},
				"inputs": [
					{
						"referenceName": "DS_REST_PAR",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_ADLS_REST_PAR",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}